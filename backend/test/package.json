{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm test --timeout 25000 --bail=50 --preload=./setup.ts",
    "start:ci": "npm run start .",
    "lint": "eslint . --cache $ESLINT_ARG",
    "lint:fix": "npm run lint --fix",
    "lint:ts": "npm run tsc --noEmit",
    "lint:ci": "npm run lint && npm run codegen:validate && npm run lint:ts && npm run lint:ts-prune",
    "lint:ts-prune": "ts-prune -e -i 'generated|codegen|npm-types.d.ts'",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:validate": "previous_hash=$(npm run --silent codegen:hash) && npm run codegen && if test $previous_hash = $(npm run --silent codegen:hash);then echo 'Types is consistent within current schema'; exit 0; else echo 'Types is not consistent within schema'; exit 1; fi",
    "codegen:hash": "echo $(find ./generated -type f -print0 | sort -z | xargs -0 sha256sum | sha256sum | cut -d \" \" -f 1);",
    "install-eslint-plugin": "cd ./eslint-plugin-custom && npm link",
    "test": "jest --config jest.config.ts ./__tests__/**/*.test.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.1",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-custom": "^0.0.6",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-request-upload": "^0.0.3",
    "graphql-ws": "^5.14.3",
    "jose": "^5.2.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "uuid": "^9.0.1"
  }
}
